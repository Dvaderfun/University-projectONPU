<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="Main">
    <children>
        <ListView fx:id="listView">
            <VBox.margin>
                <Insets left="10.0" right="10.0" top="10.0"/>
            </VBox.margin>
        </ListView>
        <HBox spacing="20.0" VBox.vgrow="NEVER">
            <children>
                <Button fx:id="button_add" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Добавить"
                        HBox.hgrow="ALWAYS"/>
                <Button fx:id="button_delete" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Удалить"
                        HBox.hgrow="ALWAYS"/>
            </children>
            <VBox.margin>
                <Insets/>
            </VBox.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </HBox>
    </children>
</VBox>
