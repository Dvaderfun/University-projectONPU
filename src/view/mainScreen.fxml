<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main">
    <children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <TextField prefHeight="31.0" prefWidth="223.0" promptText="Search...">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
        <ListView fx:id="listView" prefHeight="400.0" prefWidth="600.0" />
        <HBox spacing="20.0" VBox.vgrow="NEVER">
            <children>
                <Button fx:id="button_add" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Добавить" HBox.hgrow="ALWAYS" />
                <Button fx:id="button_delete" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Удалить" HBox.hgrow="ALWAYS" />
            </children>
            <VBox.margin>
                <Insets />
            </VBox.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
    </children>
</VBox>
