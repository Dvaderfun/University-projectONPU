<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<VBox prefHeight="554.0" prefWidth="733.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Main">
    <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
         <items>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Events of student #&amp;">
               <font>
                  <Font name="Google Sans Regular" size="15.0" />
               </font>
            </Text>
         </items>
      </ToolBar>
      <HBox prefHeight="11.0" prefWidth="600.0" spacing="20.0">
         <children>
            <Text fx:id="eventQuantity" strokeType="OUTSIDE" strokeWidth="0.0" text="Events: 15">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Text>
         </children>
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <ListView fx:id="eventList" prefHeight="462.0" prefWidth="459.0" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </ListView>
        <HBox spacing="20.0" VBox.vgrow="NEVER">
            <children>
                <Button fx:id="addEvent" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="31.0" prefWidth="1216.0" text="Add Event" HBox.hgrow="ALWAYS" />
            <Button fx:id="removeEvent" disable="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefHeight="31.0" prefWidth="1216.0" text="Remove Event" />
            </children>
            <VBox.margin>
                <Insets />
            </VBox.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
    </children>
</VBox>
